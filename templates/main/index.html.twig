	#contentContainer {
		box-sizing:border-box;

		padding-top: 30px;
		display: flex;
		justify-content: center;
		/* Centre horizontalement */
		align-items: center;
		/* Centre verticalement */
		height: auto;
		min-height: 700px;
		overflow-x: hidden;
		width: 100vw;
		transform-style: preserve-3d;
		z-index: 1;
		-webkit-box-flex:0;
		-ms-flex-preferred-size:100%;
		box-sizing:border-box;
		-ms-flex:0 0 auto;
		flex:0 0 auto;
		flex-basis:100%;
		max-width:100%;
		padding-left:8px;
		padding-right:8px


	}

	#carouselContainer {
		box-sizing:border-box;
		margin-left:auto;
		margin-right:auto;
		margin-top:auto;
		margin-bottom:auto;
		padding-left:18px;
		padding-right:18px;
		position: relative;
		width: 100vw;
		
		min-height: 700px;
		/* Ajuste selon le besoin */
		display: flex;
		justify-content: center;
		/* Centre horizontalement */
		align-items: center;
		/* Centre verticalement */
		transform-style: preserve-3d;
	}

	.container-carousel {
		box-sizing:border-box;
	
		position: relative;
		width: 100vw;
		min-height: 700px;
		display: flex;
		justify-content: center;
		align-items: center;
		transform-style: preserve-3d;
		perspective: 4000px;

	}

	#carouselItem {
		position: absolute;
		width: 150px;
		height: 250px;
		display: flex;
		justify-content: center;
		align-items: center;
		transform-style: preserve-3d;
		object-fit: contain;



	}

	.container-carousel .item {
	    width: auto;
		height: auto;
		max-width: 80vw;
		max-height: 85vh;
		justify-content: center;
		align-items: center;
		object-fit: contain;
	}

	.carouselItem {
		position: absolute;
		width: auto;
		/* Ajuste selon la taille des items */
		height: auto;
		/* Ajuste selon la taille des items */
		display: flex;
		justify-content: center;
		align-items: center;
		perspective: 6000px;
		transition: transform 0.5s ease-in-out;
		transform-style: preserve-3d;

	}

	.carousel-image img {
		position: absolute;
		align-items: center;
		top: 20;
		width: 80%;
		height: 90%;
	    right: 30px;
		object-fit: contain;
		z-index: -1;
	   
	
	  }

	#carouselItem img {
		width: 70%;
		height: 100%;
		object-fit: contain;
		align-items: center;
		opacity: 0.95;
		

	}

	#carouselItem .item h2 {
		z-index: 9;
		position: relative;
		color: rgba(184, 174, 84, 0.923);
		border-color: linear-gradient(#e3ded8, #959c92) 50;
		font-size: 1.8rem;
		text-shadow: -1px -0.5px 2px rgba(17, 17, 17, 0.712);
		font-weight: 900;
		writing-mode: vertical-rl; /* Texte écrit de haut en bas */
		text-orientation: upright; /* Permet de garder le texte droit */
		margin: 0;
		text-transform:  uppercase;
		letter-spacing: -3px;
	}
	
	#carouselItem .item p {
		font-size: 0.7rem;
		color: rgba(224, 224, 224, 0.959);
		font-weight: bold;
		text-shadow: -1.3px 0.3px 1px rgba(31, 30, 30, 0.918);
		position: relative;
		z-index: 9;
		text-orientation: mixed;  
		margin-top: 40%;
		margin-left: 10%;
		letter-spacing: 1.5px;
		text-transform:  uppercase;
	}

	h5 {
		color: rgb(154, 146, 77);
		font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
		font-size: 18px;
		text-shadow: 4px solid #FFF;
		font-weight: 600;

	}

	.btn {
		position: absolute;
		top: 80%;
		transform: translateY(-80%);
		font-size: 3rem;
		cursor: pointer;
		color: white;
		background: rgba(0, 0, 0, 0.5);
		padding: 1px;
		z-index: 12;
		user-select: none;
		transition: 0.3s;
		width: 4vw;

	}
	
	.btn:hover {
		background: rgba(0, 0, 0, 0.8);
	}
	
	/* Positionnement */
	.prev {
		left: 5px;
		position: fixed;
    }
	
	.next {
		right: 5px;
	}
	
	/* Ajustement des icônes */
	.btn i {
		font-size: 2.5rem;
		color: white;
	}

	@media (max-width: 768px) {
		.container-carousel {
			width: 220px;
			height: 250px;
			padding: auto center;
			margin-top: 15px;
			margin-bottom: 300px;
			position: relative;
			perspective: 200;
			transform-style: preserve-3d;
			transition: transform 1.5s;
			justify-content: center;
			justify-items: center;
			align-items: center;
		}

		.item {
			display: block;
			position: absolute;
			width: 200px;
			height: 250px;
			justify-content: center;
			align-items: center;
			opacity: 0.95;
			border-radius: 10px;
			justify-items: center;

		}

