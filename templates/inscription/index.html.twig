        animation: Toastify__trackProgress linear 1 forwards
    }
    
    .Toastify__progress-bar--controlled {
        transition: transform .2s
    }
    
    .Toastify__progress-bar--rtl {
        right: 0;
        left: initial;
        transform-origin: right
