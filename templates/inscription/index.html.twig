    @keyframes Toastify__trackProgress {
        0% {
            transform: scaleX(1)
        }
    
        to {
            transform: scaleX(0)
        }
    }
    
    .Toastify__progress-bar {
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 5px;
        z-index: var(--toastify-z-index);
        opacity: .7;
        transform-origin: left
    }
    
