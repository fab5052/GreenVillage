    }
    
    .Toastify__close-button {
        color: #fff;
        background: transparent;
        outline: none;
        border: none;
        padding: 0;
        cursor: pointer;
        opacity: .7;
        transition: .3s ease;
        -ms-flex-item-align: start;
        align-self: flex-start
    }
    
    .Toastify__close-button--light {
        color: #000;
        opacity: .3
