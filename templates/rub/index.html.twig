{% extends 'base.html.twig' %}

{% block title %}Hello RubricsController!{% endblock %}

{% block body %}
<style>
    .example-wrapper {
        margin: 1em auto;
        max-width: 800px;
        width: 95%;
        font: 18px/1.5 sans-serif;
    }

    .example-wrapper code {
        background: #F5F5F5;
        padding: 2px 6px;
    }
</style>

<main>
    <div class="page">
        <div class="page__content" style="max-width: 600px;">
            <div class="um-card um-card--use-padding um-card--rounded profile-form-wrapper">
                <div class="section-tabs"><a class="um-tab um-tab--active" href="/app/profile">Profile</a><a
                        class="um-tab" href="/app/security">Security</a></div>
                <form class="profile-form"><a
                        class="um-button um-button--icon-only um-button--round um-button--icon-solo um-button--primary-outlined um-button--medium profile-form__link"
                        type="secondary" id="edit-profile-button" data-testid="profile-form-edit-link"
                        href="/app/profile/edit"><svg aria-hidden="true" fill="currentColor" height="24"
                            focusable="false" viewBox="0 0 24 24" width="24" xmlns="http://www.w3.org/2000/svg"
                            class="um-button__icon">
                            <rect x="3" y="19" width="18" height="2"></rect>
                            <polygon points="21 5.8 18.2 3 7 14.2 7 17 9.8 17 21 5.8"></polygon>
                        </svg><span class="um-button__content">Edit Profile</span></a>
                    <div class="profile-form__image">
